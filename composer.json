{
    "name": "nutgram/hydrator",
    "homepage": "https://github.com/nutgram/hydrator",
    "description": "Hydrator for PHP 8.0+",
    "license": "MIT",
    "keywords": [
        "dto",
        "hydrator",
        "data-mapper",
        "mapper",
        "php8",
        "jsonmapper"
    ],
    "authors": [
        {
            "name": "Sergio Brighenti",
            "email": "sergio@brighenti.me",
            "homepage": "https://github.com/SergiX44"
        },
        {
            "name": "Anatoly Fenric",
            "email": "afenric@gmail.com",
            "homepage": "https://github.com/fenric"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/container": "^1.1 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.0",
        "illuminate/container": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "SergiX44\\Hydrator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SergiX44\\Hydrator\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "XDEBUG_MODE=coverage phpunit --coverage-text --colors=always"
        ]
    }
}
